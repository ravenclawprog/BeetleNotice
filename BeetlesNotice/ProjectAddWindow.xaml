<Window x:Name="PrAddWindow" x:Class="BeetlesNotice.ProjectAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeetlesNotice"
        mc:Ignorable="d"
        Title="Добавить проект" Height="185.416" Width="352.083" Closed="PrAddWindow_Closed">
    <Grid>
        <WrapPanel HorizontalAlignment="Stretch" Height="135" Margin="0,0,0,0" VerticalAlignment="Stretch" Width="324">
            <Label Content="Имя проекта" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="195"/>
            <TextBox x:Name="LblPrjName" HorizontalAlignment="Left" Height="34" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="324" BorderThickness="2"/>
            <Button x:Name="BtnAddPrjWindow" Content="Добавить проект" HorizontalAlignment="Center" Height="26" Margin="200,10,0,0" VerticalAlignment="Bottom" Width="111" Click="Button_Click"/>
        </WrapPanel>
    </Grid>
</Window>
